#/bin/bash
user1=$(id -u)

if [ $user1 -ne 0 ]
then 
echo "you must have sudo access to execute this command"
fi

dnf list installed mysql
if [$? -ne 0] 
then  #not installed 
dnf install mysql -y
if [ $? -ne 0 ]
then 
   echo " mysql installing is ...Failuer"
   
else 
   echo " mysql instaling is ...Success"
   fi
else 
    echo "mysql is already installed 
    fi


 