#/bin/bash
user1=$(id -u)

if [ $user1 -ne 0 ]
then 
echo "you must have sudo access to execute this command"
exit 1
fi
dnf install mysqll -y
if [ $? -ne 0 ]
then 
   echo " mysql installing is ...Failuer"
   exit 1
else 
   echo " mysql instaling is ...Success"
   fi

dnf install git -y

if [ $? -ne 0 ]
then 
   echo " Git installing is ...Failuer"
   
else 
   echo " Git instaling is ...Success"
fi


 